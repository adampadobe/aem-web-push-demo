{
  "name": "aem-web-push-demo",
  "version": "1.0.0",
  "description": "Adobe Journey Optimizer Web Push Notifications with AEM Edge Delivery Services",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p 8000 -o /push-demo.html",
    "dev": "npx aem up",
    "deploy": "bash scripts/deploy-from-cursor.sh",
    "push-content": "node scripts/google-drive-sync.js",
    "deploy:full": "npm run push-content && npm run deploy",
    "google-login": "node scripts/google-oauth-login.js",
    "copy-credentials": "node scripts/copy-google-credentials-from.js",
    "generate-vapid": "npx web-push generate-vapid-keys"
  },
  "keywords": [
    "adobe",
    "aem",
    "edge-delivery-services",
    "web-push",
    "journey-optimizer",
    "push-notifications"
  ],
  "author": "Adobe",
  "license": "Apache-2.0",
  "devDependencies": {
    "@adobe/aem-cli": "^16.16.21",
    "http-server": "^14.1.1",
    "web-push": "^3.6.7"
  },
  "dependencies": {
    "googleapis": "^171.4.0",
    "open": "^11.0.0"
  }
}
